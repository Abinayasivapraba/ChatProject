app.controller('UserController',[$rootScope,$scope,$cookieStore,$location,$firebaseArray, 
                                  function($rootScope,$scope,$cookieStore,$location,$firebaseArray){
	
	
	console.log("UserController")
	
	var ref=firebase.database().ref().child('users');
	$scope.users=$firebaseArray(ref);
	$scope.submit=function(){
		$scope.users.$add({
			username:$scope.username,
			role:$scope.role,
			email:$scope.email,
			contact:$scope.contact
			
		})
	}
	
	
	
	
	
	
	
}])